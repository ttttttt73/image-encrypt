pragma solidity >=0.4.21 <0.7.0;

contract SimpleStorage {
    
    string public symkey;
    string public iv;
    string public ipfsHash;

    event Send(string _symkey, string _iv, string _ipfsHash);

    function set(string memory x, string memory y, string memory z) public {
        ipfsHash = x;
        iv = y;
        symkey = z;
        emit Send(symkey, iv, ipfsHash);
    }

    function get() public view returns (string memory, string memory, string memory) {
        return (symkey, iv, ipfsHash);
    }

    /*function kill() public onlyOwner() {
        selfdesturct(owner);
    }*/
}